@namespace("com.databricks.parquet.avro")
protocol ParquetAvroCompat {
  record AvroIntArray {
    array<int> f;
  }

  record AvroArrayOfOptionalInt {
    array<union { null, int }> f;
  }

  record AvroArrayOfIntArray {
    array<array<int>> f;
  }

  record AvroNestedRecord {
    union { null, int } c;
    union { null, int } d;
  }

  record AvroSchemaMergingHelper {
    AvroNestedRecord n;
  }
}
